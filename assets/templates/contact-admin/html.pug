extends ../layout/index

block content
  div#contact-admin
    p.greeting
      | Hi <strong>admin</strong>, 
    p
      | There is a contact message from our customer. Detail:
      br
      ul
        li
          | From: <em>!{`${_data.name}`}</em>
        li
          | Email: <em>!{`${_data.email}`}</em>
        li
          | About: <em>!{`${_data.subject}`}</em>
        li
          | Content: <em>!{`${_data.message}`}</em>
